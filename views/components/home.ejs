<div class="m-auto flex p-10 w-full justify-center flex-col bg-neutral-100">
  <div
    class="m-auto w-4/5 p-10 rounded-lg flex justify-center flex-col border-solid border-2 bg-white border-neutral-600"
  >
    <div class="m-auto text-xl">
      <ul>
        <% allpost.forEach((data) => { %>
        <div class="m-auto p-5 flex flex-col">
          <h1 class="text-center">"<%= data.title %>"</h1>
          <hr class="bg-gray-500 p-5 mt-6" />
        </div>

        <% }); %> <% if(!allpost.length) {%>
        <h1>dont have posts</h1>
        <% } %>
      </ul>
    </div>
  </div>
</div>
